.page
	
	position absolute
	pos 0
	use-flex-center column
	
	opacity 0.25
	
	> img {
		
		width 24rem
		max-width 80%
		
	}
	
	> table {
		
		clear-pad()
		border-spacing 0
		font-size 14px
		border-spacing 0.56em 1em
		
		td:first-child {
			clear-pad()
			text-align right
		}
		
		a {
			
			color #0553b9
			text-decoration none
			
		}
		
	}
	
